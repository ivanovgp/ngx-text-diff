{
  "name": "ngx-text-diff-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration=production",
    "build_report": "ng build --configuration=production --stats-json",
    "test": "echo 1",
    "lint": "ng lint",
    "postinstall": "ngcc",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-text-diff --configuration=production",
    "copy-license": "copyfiles ./projects/ngx-text-diff/LICENSE ./dist/ngx-text-diff",
    "copy-readme": "copyfiles ./projects/ngx-text-diff/README.md ./dist/ngx-text-diff",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-text-diff && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack && mv dist/ngx-text-diff/ngx-text-diff-*.tgz dist/ngx-text-diff.tgz"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.52.2",
    "copyfiles": "^2.2.0",
    "core-js": "^3.6.5",
    "diff-match-patch": "^1.0.4",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "rxjs": "^6.6.2",
    "tslib": "^2.5.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@types/codemirror": "0.0.91",
    "@types/diff-match-patch": "^1.0.32",
    "@types/express": "^4.17.6",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.55",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^14.2.1",
    "prettier": "^2.7.1",
    "protractor": "^7.0.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.1",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
